@meta {
  import (
    "github.com/mgutz/gorazor/example/views/layout"
    "github.com/mgutz/gorazor/example/models"
  )

  +func(user *models.User)
  +return layout.Default(VIEW, gorazor.Empty, js(), title)
}

@{
  title := "Cool Site"
}

<p>Escaped: @UnsafeHello(user.Name)</p>
<p>Unescaped: @SafeHello(user.Name)</p>

@Raw("<h2>Heading 2</h2>")

@section js {
  <script>
    alert('Hello, @user.Name');
  </script>
}


